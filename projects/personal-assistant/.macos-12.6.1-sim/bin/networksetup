#!/usr/bin/env bash
set -euo pipefail
if [[ "$1" == "-listallhardwareports" ]]; then
  cat <<'OUT'
Hardware Port: Wi-Fi
Device: en0
Ethernet Address: aa:bb:cc:dd:ee:ff
OUT
  exit 0
fi
if [[ "$1" == "-getairportnetwork" && -n "${2:-}" ]]; then
  echo "Current Wi-Fi Network: TestWifi"
  exit 0
fi
echo "networksetup stub: unsupported args $*" >&2
exit 1
